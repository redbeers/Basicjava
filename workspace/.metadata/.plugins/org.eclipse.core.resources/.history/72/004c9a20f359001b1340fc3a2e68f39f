package g_exception;

public class Exception_04 {

	public static void main(String[] args) {

		Exception ee = new Exception("선생님때문이야~~~");
		try{	
			
			throw ee;
		}catch(Exception e){
			e.printStackTrace();
		}
		System.out.println("프로그램 종");

		RuntimeException run = new RuntimeException("😎");
		
		
			throw run;
		
	}

}

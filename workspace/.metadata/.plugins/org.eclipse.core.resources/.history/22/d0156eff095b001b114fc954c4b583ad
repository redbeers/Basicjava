package store;

import java.util.Vector;

public class Guest {
	String name;
	int money;
	int miles;
	
	Vector<Guest> people = new Vector<>();
	
	public Guest(String name, int money ) {
		this.name = name;
		this.money = money;
		miles = money/10;
	}
	public void storage(Guest g){
		people.add(g);
	}
	
}

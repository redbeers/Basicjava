package store;

import java.util.Vector;

public class Guest {
	String name;
	int money;
	int miles;
	
	public Guest(String name, int money, int susu) {//초기화
		this.name = name;
		this.money = money;
	}
}


class OutGuest{
	
	Vector<Guest> people = new Vector<>();//백터 사람관리
	
	public void storage(Guest q){//백터에 저장
		people.add(q);
		System.out.println(q.name+"님 안녕하세요");	
	}

	
}